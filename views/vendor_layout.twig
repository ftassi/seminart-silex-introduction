<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head>

    {% block meta_tags %}
    <meta name="Description" content="Information architecture, Web Design, Web Standards." />
    <meta name="Keywords" content="your, keywords" />
    <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
    <meta name="Distribution" content="Global" />
    <meta name="Author" content="Erwin Aligam - ealigam@gmail.com" />
    <meta name="Robots" content="index,follow" />
    {% endblock %}

    <link rel="stylesheet" href="/css/CoolWater.css" type="text/css" />

    {% block stylesheets %}{% endblock %}
    {% block javascripts %}{% endblock %}

    <title>{% block title %}{% endblock %}</title>
	
</head>

<body>
<!-- wrap starts here -->
<div id="wrap">
		
	<!--header -->
	<div id="header">			
				
		<h1 id="logo-text">{% block logo_text %}<a href="#">Main Title</a>{% endblock %}</h1>		
		<p id="slogan">{% block slogan %}website slogan{% endblock %}</p>		
			
		<div id="header-links">
			<p>
            {% block header_links %}
			<a href="index.html">Home</a> | 
			<a href="index.html">Contact</a> | 
			<a href="index.html">Site Map</a>			
            {% endblock %}
			</p>		
		</div>		
						
	</div>
		
	<!-- navigation -->	
	<div  id="menu">
        {% block menu %}
		<ul>
			<li id="current"><a href="index.html">Home</a></li>
			<li><a href="index.html">Archives</a></li>
			<li><a href="index.html">Downloads</a></li>
			<li><a href="index.html">Services</a></li>
			<li><a href="index.html">Support</a></li>
			<li><a href="index.html">About</a></li>		
		</ul>
        {% endblock %}
	</div>					
			
	<!-- content-wrap starts here -->
	<div id="content-wrap">
		{% block body %}

		<div id="main">				
            {% block main_content %}
